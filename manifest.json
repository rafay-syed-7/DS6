{
    "manifest_version": 3,
    "name": "Pulse",
    "description": "Productivity Timer.",
    "version": "1.0",
    "permissions": ["storage", "alarms"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16-blue.png"
      }
    }
  }
  